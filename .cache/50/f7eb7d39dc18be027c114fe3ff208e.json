n      font-size: 5ch;\n    }\n  \n    @keyframes animate\n    {\n      0%\n      {\n        transform: rotate(0deg);\n      }\n      100%\n      {\n        transform: rotate(360deg);\n      }\n    }\n  }\n}\n        \n          .fade-out{\n              z-index: -1;\n                opacity:0;\n            }","scss/main.scss":"@use \"abstracts\" as *;\n@use \"components\";\n\n\nhtml{\n    font-size: 62.5%;\n    overflow: hidden;\n    margin: 0;\n    padding: 0;\n    box-sizing: border-box;\n    font-family: 'Gotham Rounded', sans-serif;\n    text-decoration: none;\n    color: white;\n}\n  \nbody{\n  height: 100vh;\n  width: 100vw;\n  display: flex;\n  justify-content: center;\n  flex-direction: column;\n  align-items: center;\n  background-color: #70a1ff;\n  background-image: linear-gradient(315deg, #70a1ff 0%, #c2c0c0 74%);\n}\n      \n\n\n "},"lineCount":null}},"error":null,"hash":"57cefff14509e0d80e1f9da06a6a8f2a","cacheData":{"env":{}}}